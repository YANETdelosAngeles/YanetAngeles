/*=============== SCROLL REVEAL ANIMATION ===============*/
const sr = ScrollReveal({
    origin: 'top',
    distance: '60px',
    duration: 2500,
    delay: 0,
})

sr.reveal(`#bloque1 , #bloque1 p`)
sr.reveal(`#bloque2`,{delay: 500})
sr.reveal(`h2`, {interval: 100, delay: 500})
sr.reveal(`.cinta .col-fixed , .cinta>p`, {interval: 100,delay: 600})